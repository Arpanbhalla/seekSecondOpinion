
<% if logged_in? %>
    <%= link_to 'Log out', logout_path, method: :delete %><strong> | </strong>
    <!-- <%#= link_to "Mailbox (#{unread_messages})", conversations_path, class: "item" %><strong> | </strong> -->

    <% if is_doctor? %>
    <%= link_to "View your profile", user_path(current_user) %>
    <% else %>
      <% if is_admin? %>
        <%= link_to 'Create Doctor Profile', new_user_path %><strong> | </strong>
      <%end%>
        <%= link_to "View your profile", user_path(current_user) %><strong> | </strong>
        <%= link_to "All Doctors", users_path %><strong> | </strong>
        <%= link_to "Home", root_path %><strong> | </strong>
    <% end %>
<% else %>

  <%= link_to 'Log in', login_path %> <strong> | </strong>
  <%= link_to 'Sign up', new_user_path %>

  <% end %>
