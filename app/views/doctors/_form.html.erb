<%= form_for @doctor do |f| %>

   <%= f.label :image %>
  <%= f.url_field :image%><br>

  <%= f.label :fullname %>
  <%= f.text_field :fullname %><br>

  <%= f.label :qualifications %>
  <%= f.text_field :qualifications %><br>

  <%= f.label :speciality %>
  <%= f.text_field :speciality %><br>

  <%= f.label :professional_experience %>
  <%= f.text_field :professional_experience %><br>

  <%= f.label :awards_recognitions %>
  <%= f.text_field :awards_recognitions %><br>

  <%= f.label :memberships %>
  <%= f.text_field :memberships %><br>

  <%= f.label :publications %>
  <%= f.text_field :publications %><br>

  <%= f.label :email %>
  <%= f.email_field :email %><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %><br>

  <%= f.submit "Sign up" %>
<% end %>
