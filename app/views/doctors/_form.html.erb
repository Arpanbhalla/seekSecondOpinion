<%= form_for @doctor do |f| %>

   <%= f.label :image %>
  <%= f.url_field :image%><br>

  <%= f.label :doctors_fullname %>
  <%= f.text_field :doctors_fullname %><br>

  <%= f.label :doctors_qualifications %>
  <%= f.text_field :doctors_qualifications %><br>

  <%= f.label :doctors_speciality %>
  <%= f.text_field :doctors_speciality %><br>

  <%= f.label :doctors_professional_experience %>
  <%= f.text_field :doctors_professional_experience %><br>

  <%= f.label :doctors_awards_recognitions %>
  <%= f.text_field :doctors_awards_recognitions %><br>

  <%= f.label :doctors_memberships %>
  <%= f.text_field :doctors_memberships %><br>

  <%= f.label :doctors_publications %>
  <%= f.text_field :doctors_publications %><br>

  <%= f.label :doctors_email %>
  <%= f.email_field :doctors_email %><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %><br>

  <%= f.submit "Sign up" %>
<% end %>
